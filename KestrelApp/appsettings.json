{
  "Kestrel": {
    "Endpoints": {
      "Echo": { // echo服务器
        "Url": "http://localhost:5000"
      },
      "XorEcho": { // xor流量处理的echo服务器
        "Url": "http://localhost:5001"
      },
      "XorEchoProxy": { // 将Echo流量Xor处理然后转发到XorEcho服务器的代理服务器
        "Url": "http://localhost:5002"
      },
      "HttpProxy": { // http代理服务器
        "Url": "http://localhost:5003"
      },
      "HttpHttps": { // http和https双协议服务器
        "Url": "http://localhost:5004"
      },
      "EchoWebSocket": { // echo over websocket
        "Url": "http://localhost:5005"
      }
    },
    "Certificates": {
      "Default": {
        "Path": "",
        "Password": ""
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Yarp": "Warning",
        "System": "Warning",
        "Microsoft": "Warning",
        "Microsoft.Hosting": "Information"
      }
    }
  }
}